<?php
include "config.php";
if ($db == false){
    die("unable to connect to db:<br>");
}
$dbquery = "SELECT * FROM AUTHORIZED_USERS WHERE USERNAME = 'NKOHN'";
$dbresult = mysqli_query( $db, $dbquery);

if ($dbresult == false){
    die("unable to perform query");
}
while($dbrow = mysqli_fetch_array($dbresult){
    print_r($dbrow);
    echo'<br>';
    
}

if (mysqli_connect_errno()) {
    
    echo "Could  not connect to database: Error: ".mysqli_connect_error();
    
    exit();
}
?>