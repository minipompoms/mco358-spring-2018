<?php
session_start();

// Create connection
$conn = mysqli_connect("127.0.0.1", "admin", "password", "myHarvest");

if (!$conn) {
    die("Connection failed: " . mysqli_connect_error());
}
$sql = "SELECT username, password FROM AUTHORIZED_USERS";
$result = mysqli_query($conn, $sql);

if (mysqli_num_rows($result) > 0) {
    // output data of each row
    while($row = mysqli_fetch_assoc($result)) {
        echo "username: " . $row["username"]. $row["password"]. "<br>";
    }
} else {
    echo "0 results";
}

mysqli_close($conn);

?>

<?php include "header.php";?>

<body>

<h2>Account Profile</h2><br><br>
<p><canvas style=height:300px;></canvas>


</body>
<?php include "footer.php";?>